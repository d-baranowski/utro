{
  "name": "utro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
    "db:generate": "drizzle-kit generate --config=./drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=./drizzle.config.ts",
    "db:migrate": "cross-env DB_MIGRATING=true tsx src/db/migrate.ts",
    "db:sync": "cross-env pnpm run db:generate && pnpm run db:migrate",
    "db:add-migration": "drizzle-kit generate --custom --name='${npm_config_foo}' --config=./drizzle.config.ts",
    "db:seed": "cross-env DB_SEEDING=true tsx src/db/seed.ts"
  },
